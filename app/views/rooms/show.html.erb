
<div class="page-header">

  <h1>Room: <%= @room.name %></h1>
</div>

<div id="videos"
     class="embed-responsive embed-responsive-1by1"
     data-controller="vonage"
     data-vonage-api-key="<%= ENV.fetch('VONAGE_API_KEY', Rails.application.credentials.vonage_api_key) %>"
     data-vonage-session-id="<%= @room.vonage_session_id %>"
     data-vonage-token="<%= @token %>"
     data-vonage-name="<%= current_user.email %>"
>
</div>